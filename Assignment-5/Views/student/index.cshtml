@model IEnumerable<Assignment_5.Models.Student>

@{
    ViewData["Title"] = "Student List";
}

<h2>Student List</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Course</th>
        <th>Age</th>
     
    </tr>

    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Id</td>
            <td>@student.Name</td>
            <td>@student.Course</td>
            <td>@student.Age</td>
        </tr>
    }
</table>
